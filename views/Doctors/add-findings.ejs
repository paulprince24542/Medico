        <script>
            const queryString = window.location.search;
            console.log(queryString);
            const urlParams = new URLSearchParams(queryString);
            console.log(urlParams.get('patientid'))


            async function submitFinding() {
                var findings = document.getElementById("findings").value
                var prescriptions = document.getElementById("prescriptions").value
                console.log(findings, prescriptions)
                const response = await fetch("/doctor/private/addfindings", {
                    method: "POST",
                    mode: "cors",
                    cache: "no-cache",
                    credentials: "same-origin",
                    headers: {
                        "Content-Type": "application/json",

                    },
                    body: JSON.stringify({
                        appointmentid: urlParams.get('appointmentid'),
                        patientid: urlParams.get('patientid'),
                        doctorid: urlParams.get('doctorid'),
                        findings: findings,
                        prescriptions: prescriptions,
                        active: document.getElementById("active").value
                    })
                });
                const jsonData = await response.json();
                console.log(jsonData);
                if (jsonData.msg) {
                    $("#simpleModal").modal('show');

                }
                console.log()

            }

            function d_Redirect() {
                setTimeout(() => {
                    window.location.href = "/client/doctor/appointments"
                }, 3000)
            }

            function logout(){
                window.location.href = "/client/patient/login"
            }
        </script>
