    <script>
        async function getDoctor() {
            var ima_number = document.getElementById("imanumber").value
            console.log(ima_number)
            var res = await fetch("/hospital/private/getdoctor", {
                method: "POST",
                mode: "cors",
                cache: "no-cache",
                credentials: "same-origin",
                headers: {
                    "Content-Type": "application/json",

                },
                body: JSON.stringify({
                    imano: ima_number,
                })

            })
            var jsonResponse = await res.json()
            // console.log(jsonResponse)
            document.getElementById("name").value = jsonResponse.data[0].firstname + " " + jsonResponse.data[0].lastname
            document.getElementById("department").value = jsonResponse.data[0].specialization
        }


        async function assignDoctor() {
            var ima_number = document.getElementById("imanumber").value
            var res = await fetch("/hospital/private/assigndoctor", {
                method: "POST",
                mode: "cors",
                cache: "no-cache",
                credentials: "same-origin",
                headers: {
                    "Content-Type": "application/json",

                },
                body: JSON.stringify({
                    imano: ima_number,
                })

            })
            var jsonResponse = await res.json()
            console.log(jsonResponse)
            $("#simpleModal").modal('show');
        }

        function d_Redirect() {
            setTimeout(() => {
                window.location.href = "/client/hospital/doctors"
            }, 1000)
        }
    </script>
